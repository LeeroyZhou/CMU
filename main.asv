Ans=[]
for i=8:0.5:12 %多起点进行最小值搜索
    for j=1:1:15
        [i,j]
        [x,out]=fminsearch('func1',[i,j,i-1]);
        if out<min
            real=[x,out];
            min=out;
            if out-0.1016 < 0.1
                Ans=[Ans, real]
            end
        end
    end
end

